@model RentaMaq.Models.alertasMantenimiento

@{
    ViewBag.Title = "eliminar alerta de correo";
}

<h2>Eliminar alerta de correo de mantenimiento</h2>

<h3>¿Está seguro/a de eliminar la alerta de correo?</h3>
<div>
    <h4>Datos:</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.nombre)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombre)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.correo)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.correo)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
